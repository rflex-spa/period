parameters:

    paths:
        - src
        - tests

    level: 6

    reportUnmatchedIgnoredErrors: false

    ignoreErrors:
        - '#Parameter \#1 \$year of static method Carbon\\Carbon::create\(\) expects DateTimeInterface\|int\|null, string given.#'

    excludePaths:
        - ./src/PeriodServiceProvider.php

    checkMissingIterableValueType: false
    checkGenericClassInNonGenericObjectType: false
