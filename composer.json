{
    "name": "rflex/period",
    "description": "Customized library for comparing periods easily. This is an extension of Carbon\\Period.",
    "type": "library",
    "authors": [
        {
            "name": "David Miranda Atenas",
            "email": "dmiranda@rflex.cl"
        }
    ],
    "require": {
        "nesbot/carbon": "^2.54"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Rflex\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rflex\\PeriodServiceProvider"
            ]
        }
    }
}
